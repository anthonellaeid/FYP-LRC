* {
    box-sizing: border-box;
  }
   ul{border-bottom:2px solid black;
      border-top:1px solid black;
      background-color:white;}
  
  img{border-radius:60px;}
  
  a:link, a:visited {
    color: black;
    padding: 10px 20px;
    text-align: center;
    display: inline-block;}
  a:hover, a:active {
    background-color: white;
    color: red;}
  /* Create two unequal columns that floats next to each other */
  .column {
    float: left;
    padding: 10px;
     /* Should be removed. Only for demonstration */
    border:2px solid white;
  }
  
  .left {margin-left:2%;
    width: 30%;
    height: 350px;
  }
  
  .right {
    width: 65%;
    height:auto;
  }
  
  /* Clear floats after the columns */
  .row:after {
    content: "";
    display: table;
    clear: both;
  }
  
  

    
  .c1{margin:4%;
      width:auto; }
  #A{margin-top:7%;}